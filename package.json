{
  "name": "hybridispeksi",
  "version": "2.0.1",
  "main": "server.js",
  "author": "pymapa, tatahe",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config ./webpack/prod.config.js --progress --colors",
    "dev": "webpack-dev-server --config ./webpack/dev.config.js",
    "server": "nodemon ./src/server",
    "start": "node src/server",
    "lint": "./node_modules/.bin/eslint src",
    "lint-fixup": "./node_modules/.bin/eslint src --ext .js,.jsx --fix",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage",
    "test": "jest",
    "test watch": "jest -- --watchAll"
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.1",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "css-loader": "^0.28.5",
    "cuid": "^2.1.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.0.1",
    "moment": "^2.18.1",
    "mongoose": "^4.10.8",
    "morgan": "^1.8.2",
    "nodemailer": "^4.4.2",
    "nodemailer-mailgun-transport": "^1.4.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-loadable": "^5.5.0",
    "react-moment": "^0.6.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.3",
    "sha1": "^1.1.1",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-combine-loaders": "^2.0.4",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^0.11.2",
    "flow": "^0.2.3",
    "flow-bin": "^0.42.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "prettier-eslint": "^8.8.1",
    "react-test-render": "^1.1.1",
    "webpack-dev-server": "^3.1.8"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/client/__config__/test-shim.js",
      "<rootDir>/client/__config__/test-setup.js",
      "<rootDir>/client/__config__/test-env.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
